const mongoose = require("mongoose")

const Schema = mongoose.Schema

const ContactUsSchema = new Schema(
    {
        first_name: String,
        email: String,
        subject: String,
        your_message: String,
        is_deleted: {
            type: Boolean,
            default: false
        }
    },
    { timestamps: true }
)

module.exports = mongoose.model("contactUs", ContactUsSchema)
